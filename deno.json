{
  "imports": {
    "lume/": "https://cdn.jsdelivr.net/gh/lumeland/lume@3f2f37e424b6e17c178a026f23e74e7a7f3b3494/",
    "axiod/mod": "https://deno.land/x/axiod@0.26.2/mod.ts",
    "axiod/interfaces": "https://deno.land/x/axiod@0.26.2/interfaces.ts",
    "typewriter": "https://unpkg.com/typewriter-effect@2.21.0/dist/core.js",
    "xodium/constants": "./src/assets/modules/xodium.constants.ts",
    "xodium/typewriter": "./src/assets/modules/xodium.typewriter.ts",
    "xodium/utils": "./src/assets/modules/xodium.utils.ts",
    "xodium/utils/github": "./src/assets/modules/xodium.utils.github.ts",
    "xodium/utils/localstorage": "./src/assets/modules/xodium.utils.localstorage.ts"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
